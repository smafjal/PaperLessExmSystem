<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestSetInfo</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.TestSetInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestInfoVerification</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.TestInfoVerification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestEntryVerification</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.TestEntryVerification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionsAdd</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.QuestionsAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExamEvalute</servlet-name>
        <servlet-class>sust.paperlessexm.servlets.ExamEvalute</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestSetInfo</servlet-name>
        <url-pattern>/TestSetInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestInfoVerification</servlet-name>
        <url-pattern>/TestInfoVerification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestEntryVerification</servlet-name>
        <url-pattern>/TestEntryVerification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionsAdd</servlet-name>
        <url-pattern>/QuestionsAdd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExamEvalute</servlet-name>
        <url-pattern>/ExamEvalute</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            300
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
